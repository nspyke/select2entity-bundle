{
  "name": "nspyke/select2entity-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony bundle that integrates Select2 as a drop-in replacement for a standard entity field on a Symfony form.",
  "keywords": [
    "symfony",
    "select2",
    "typeahead",
    "autocomplete"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ross Keatinge",
      "email": "tetranz@gmail.com"
    },
    {
      "name": "Nik Spijkerman",
      "email": "nspyke@github.com"
    }
  ],
  "scripts": {
    "lint": "php-cs-fixer fix --dry-run --diff",
    "fix": "php-cs-fixer fix --using-cache=no"
  },
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^2.10",
    "twig/twig": "^3.0",
    "symfony/property-access": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/http-kernel": "^6.4|^7.0",
    "symfony/form": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/routing": "^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "extra": {
    "branch-alias": {
    }
  }
}
